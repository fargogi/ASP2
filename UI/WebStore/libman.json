{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@2.2.0",
      "destination": "wwwroot/jquery/"
    },
    {
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/font-awesome/",
      "files": [
        "css/font-awesome.min.css",
        "css/font-awesome.css",
        "css/font-awesome.css.map",
        "fonts/fontawesome-webfont.eot",
        "fonts/fontawesome-webfont.svg",
        "fonts/fontawesome-webfont.ttf",
        "fonts/fontawesome-webfont.woff",
        "fonts/fontawesome-webfont.woff2",
        "fonts/FontAwesome.otf"
      ]
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@3.3.7",
      "destination": "wwwroot/bootstrap"
    },
    {
      "provider": "unpkg",
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/jquery/jquery-ajax-unobtrusive/",
      "files": [
        "dist/jquery.unobtrusive-ajax.min.js",
        "dist/jquery.unobtrusive-ajax.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/jquery/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/jquery/jquery-validate/"
    },
    {
      "library": "jquery-loading-overlay@2.1.6",
      "destination": "wwwroot/jquery/jquery-loading-overlay/"
    }
  ]
}